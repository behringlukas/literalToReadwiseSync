{
  "manifest_version": 3,
  "name": "Literal to Readwise Sync",
  "description": "Sync your highlights from your books on Literal.club to Readwise.",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ]
}